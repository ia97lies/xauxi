CLIENT
  _LOOP 100 count
    _REQ localhost 8080
    __GET /$count HTTP/1.1
    __Host: localhost:8080
    __User-Agent: httest
    __
    _EXPECT . "HTTP/1\.1 200 OK"
    _WAIT
  _END
END

SERVER 8081
  _LOOP 100 count
    _RES
    _EXPECT headers "GET /$count HTTP/1\.1"
    _WAIT
    __HTTP/1.1 200 OK
    __Content-Type: text/plain
    __
  _END
END
